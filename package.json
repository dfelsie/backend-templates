{
  "name": "backend-templates",
  "version": "1.0.0",
  "description": "Templates for backend web apps",
  "main": "src/dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "fake": "ts-node --esm src/fake.ts",
    "clearData": "ts-node --esm src/clearData.ts",
    "dockdev": "npx prisma migrate deploy && ts-node --esm src/fake.ts && concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^8.2.1",
    "@prisma/client": "^4.2.1",
    "argon2": "^0.28.5",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "ioredis": "^5.0.5",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.5",
    "passport": "^0.5.3",
    "passport-local": "^1.0.0",
    "redis": "^4.1.0",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@types/connect-redis": "^0.0.18",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/ioredis": "^4.28.10",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.34",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.4",
    "@types/yargs": "^17.0.10",
    "concurrently": "^7.2.0",
    "nodemon": "^2.0.16",
    "prisma": "^4.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  }
